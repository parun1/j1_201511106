package com.sd.w10;

public class Tuner {
  String decription;
  Amplifer amplifer;
  Double frequency;
  
  public Tuner(String decription, Amplifer amplifer){
    this.decription=decription;
  }
  public void on(){
    System.out.prinln("on");
  }
  public void off(){
    System.out.prinln("off");
  }
  public void setFrequency(double frequency){
    System.out.prinln(decription+frequency);
    this.frequency=frequency;
  }
  public String toString() {
    return description
  }
}