package com.sd.sh;

public class Book {
  String description;
  Door door;
  Pencil pencil;
  public Book(String description){
    this.description=description;
  }
  public void open(){
    System.out.println(description+"open");
  }
  public void close(){
    System.out.println(description+"close");
  }
  public void setPencilPencil pencil){
    System.out.println(description+pencil);
  }
  public void setDoor(Door door){
    System.out.println(description+door);
  }
  public String toString() {
    return description;
  }
}