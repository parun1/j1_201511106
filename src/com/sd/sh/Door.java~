package com.sd.sh;

public class Door{
  String description;
  Book book;
  public  Door(String description,Book book){
    this.description=description;
    this.book=book;
  }
  public void open(){
    System.out.println(description+"open");
  }
  public void close(){
    System.out.println(description+"close");
  }
  public String toString() {
    return description;
  }
}
    